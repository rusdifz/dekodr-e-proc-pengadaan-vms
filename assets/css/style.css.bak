body{
	color: #4C5760;
	font-family: Arial, sans-serif;
	font-size: 14px;
}
ol,ul{
	list-style: none;
}
a{
	color: #0091D2;
} 
form{
	margin: 0;
}
.col-14{
	width: 25%;
	display: inline-block;
	padding: 0px 15px;
	float: left;
	box-sizing: border-box;
}
.col-34{
	width: 75%;
	display: inline-block;
	padding: 0px 15px;
	float: left;
	box-sizing: border-box;
}
.main{
	min-height: 500px;
}
#emailTambah{
	display: none;
}
.mainInner{
	display: table;
    width: 100%;
    min-height: 500px;
    table-layout: fixed;
}
.mainInner .container{
	display: table-row;
}
	.yt-loader{
	height: 3px;
	width: 100;
	display: block;
	background: blue;
}
.colorBox{
	width: 10px;
	height: 10px;
	display: inline-block;
}
.graphBar{
	margin-bottom: 20px;
	position: relative;
}
.graph{
	display: inline-block;
	background-color: blue;
	height: 20px;
	float: left;
	cursor: pointer;
}
.graphBarGroup{
	display: inline-block;
	position: relative;
}
.graphLegend{
	width: 20%;
	display: inline-block;
	float: left;
}
.graphWrap,
.graphWrapLine
{
	width: 100%;
	display: inline-block;
}
.graphWrapLine{
	position: absolute;
	top: 7px;
}
.graphWrapLine .graph{
	height: 8px;
	   

}
.graphWrap .graph,
.graphWrapLine .graph{
	
}
.graphLegend p,
.graphWrap p{
	margin: 5px 0px;
}
.graphWrap .graph{
	height: 30px;
}
.graphWrapLine .graph{
	height: 15px;
}
.msgBlock{
	padding: 10px;
	border: 1px solid #cfcfcf;
	margin-bottom: 20px;
}
.msgBlock ul{
	padding: 0;
	list-style: none;
}
.msgBlock.successVer{
	border: #1E824C 1px solid;
    color: #1E824C;
    padding: 10px 10px;
    background-color: rgb(224, 238, 230);
}
.msgBlock.warnVer{
    border: #f39c12 1px solid;
    color: #f39c12;
    padding: 10px 10px;
    background-color: rgba(243, 156, 18, 0.09);
}
.msgBlock.warnVer a{
	color: #f39c12;
	margin-bottom: 2px;
}
.msgBlock.errorVer{
    border: #c0392b 1px solid;
    color: #c0392b;
    padding: 10px 10px;
    background-color: rgba(192, 57, 43, 0.09);
}
.msgBlock.errorVer a{
	color: #c0392b;
	margin-bottom: 2px;
}
.input-form input[type="checkbox"],
.input-form input[type="radio"]{
	width: 13px;
    height: 13px;
    padding: 0;
    margin: 0;
    vertical-align: bottom;
    position: relative;
    top: -6px;
}
.actionBlock .fa{
	margin-right: 5px;
}
.actionBlock button[type='submit']{
	color: #0091D2;
	background: none;
	border: none;
}
/*=================
	Header Area
=================*/
.header{
	border-bottom: 1px solid #DCDCDC;
}
.header .container{
	padding: 0 60px;
}
.logo{
	display: block;
	float: left;
	margin: 20px 0px;
}
.logo img{
	
	height: 60px;
}
.backButton{
	display: block;
	float: right;
}
.backButton a{
	text-decoration: none;
	vertical-align: bottom;
	color: #667E8A;
}
.backButton ul{

}
.ddMenu{
	list-style: none;
	margin: 0;
	padding: 0;
}
.ddMenu > li{
	display: inline-block;
	float: left;
	display: relative;	
}
.ddMenu > li a{
	padding:0px 10px;
}
.ddMenu > li.fl a{
	
	border-right: 1px solid #667E8A;
}
.ddMenu  > li .ddSubMenu{
	display: none;
}
.ddMenu:hover > li .ddSubMenu{
	display: block;
	}
.ddMenu .fa {
    padding-right: 5px;
}

/*=================
	Admin Page
==================*/
.navMenu{
	margin: 0;
	padding: 0;
	height: 100px;
}
.navMenu li{
	display: inline-block;
	position: relative;
	float: left;
	padding: 50px 0;
}
.navMenu > li > a{
	padding: 0px 20px;
	border-right: 1px solid #667E8A;
}
.navMenu li.last a{
	border-right: none;
}
.navMenu .navMenuDD{
	position: absolute;
	padding: 0;
	top:80px;
	display: none;
	-moz-box-shadow: 0px 0px 1px #888;
    -webkit-box-shadow: 0px 0px 1px #888;
    box-shadow: 0px 0px 1px #888;
    border: 1px solid #cfcfcf;
    z-index: 99;
}
.navMenu > li:hover > .navMenuDD{
	display: block;
}		
.navMenu .navMenuDD li{
	height: 30px;
	line-height: 30px;
	padding: 10px;
	width: 250px;
	background: #fff;
	
}

.loginInfo{
    color: #ECECEC;
    text-align: center;
    padding: 0px 20px;
    margin-top: 100px;
}
.loginInfo h2{
	font-weight: normal;

}

.tab {
    padding: 0px 20px;
}
.tab .tabNav{
	margin-bottom: 0;
}
.tab .tabNav ul{
	margin: 0;
	background-color: #1E8BC3;
	/* border: 1px #1E8BC3 solid; */
    padding: 0;
}
.tab .tabNav ul li{
	display: inline-block;
	padding: 10px;
}
.tab .tabNav ul li.active{
	background-color: #ecf0f1;
}
.tab .tabNav ul li a{
	color: #fff;
	text-decoration: none;
}
.tab .tabNav ul li a:hover{
	opacity:0.7;
}
.tab .tabNav ul li.active a{
	color: #2c3e50;
}
.tab.procView{
	padding: 0px;
}

.pengadaanStep label{
	display: block;
	padding: 10px;
	border: 1px solid #cdcdcd;
	cursor: pointer;
}
.pengadaanStep label:hover{
	background: #cdcdcd;
}
.pengadaanStep label input{
	float: right;
}
/*=================
	Login Page
==================*/
.bgSh{
	width: 600px;
    margin: 100px auto 0px;
    padding-bottom: 100px;
}
.loginTitle{
	margin-bottom: 40px;
}
.loginTitle h1{
	font-weight: 100;
	text-align: center;
	margin-bottom: 0px;
}
.loginTitle p{
	margin: 0;
	text-align: center;
	font-size: 0.8em;
}
.loginBox{
	margin: 0 auto;
}
.logoLogin{
	margin: 0 auto;
}
.logoLogin img{
	display: block;
