.tableData{
	min-width: 100%;
}
.tableData td{
	white-space: nowrap;
}
.tableData thead{
	border-bottom: 2px solid #1E8BC3;
}
.tableData thead tr td{
	/*background-color: #1E8BC3;*/
	padding: 20px 15px;
	
}
