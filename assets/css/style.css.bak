	position: relative;
	display: none;
}
.compareWrapper.active{
	display: block;
}
.compareList{
	position: absolute;
	max-height: 500px;
	overflow-y:auto;
	right: 0px;
	background: #fff;
	-moz-box-shadow: 0px 0px 1px #888;
    -webkit-box-shadow: 0px 0px 1px #888;
    box-shadow: 0px 0px 1px #888;
    border: 1px solid #cfcfcf;
    width: 300px;
}
