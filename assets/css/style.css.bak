	cursor: pointer;
}
.compareList ul{
	padding: 0;
	margin: 0;
}

.compareList ul li{
	display: block;
	width: 100%;
	overflow: hidden;
	padding: 5px;
	box-sizing: border-box;
}
.compareList ul li:hover{
	background: #EAEAEA;
}
.compareList ul li > div{
	width: 100%;
}
.compareList ul li > div > *{
	float: left;
}
